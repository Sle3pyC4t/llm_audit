# LLM provider settings (openai, anthropic, deepseek)
LLM_PROVIDER=deepseek

# API keys for different providers
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Model to use for different agents (defaults shown, will use provider's default if not specified)
SOFTWARE_ENGINEER_MODEL=deepseek-chat
AUDIT_ENGINEER_MODEL=deepseek-reasoner
PENETRATION_ENGINEER_MODEL=deepseek-reasoner
REPORT_ENGINEER_MODEL=deepseek-chat

# Knowledge base settings
KNOWLEDGE_BASE_PATH=./knowledge_base

# Reporting settings
REPORT_OUTPUT_DIR=./reports 